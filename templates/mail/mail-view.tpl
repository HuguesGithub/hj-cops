<div class="card card-primary card-outline mailbox-read">
    <div class="card-header">
        <h3 class="card-title">%1$s</h3>
        <div class="card-tools">
            <a href="%2$s" class="btn btn-tool %3$s" title="Précédent"><i class="fa-solid fa-chevron-left"></i></a>
            <a href="%4$s" class="btn btn-tool %5$s" title="Next"><i class="fa-solid fa-chevron-right"></i></a>
        </div>
    </div>
    <div class="card-body p-0">
        <div class="mailbox-read-info">
            <h5>%6$s</h5>
            <h6>De : %7$s <span class="mailbox-read-time float-end">%8$s</span></h6>
        </div>

        <div class="mailbox-controls with-border text-center">%9$s</div>

        <div class="mailbox-read-message">%10$s</div>
    </div>
