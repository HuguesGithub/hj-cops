                <textarea class="form-control text-xs" aria-label="Description" readonly="readonly">%2$s</textarea>
            </div>
            <div class="input-group mb-3">
                <span class="input-group-text col-10">Adrénaline</span>
                <input type="text" class="form-control col-2 text-white %3$s" aria-label="Adrénaline" aria-describedby="Adrénaline" value="%4$s">
